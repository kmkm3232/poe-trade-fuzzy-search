{
  "manifest_version": 3,

  "name": "automatic fuzzy search in poe trade",
  "description": "This is description.",
  "version": "1.1",
  "action": {
    "default_icon": {"01":"logo.png"},
    "default_popup": "popup.html",
    "default_title": "Automatic fuzzy search in poe trade!"
  },
  "icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "48": "logo48.png",
    "64": "logo64.png",
    "128": "logo128.png"
  },
  "content_scripts":[
    {
      "matches": ["https://www.pathofexile.com/trade/*"],
      "js" : ["script.js"]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}